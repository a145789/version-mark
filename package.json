{
  "name": "version-mark",
  "version": "0.0.0",
  "description": "A version marking tool suitable for Web projects",
  "keywords": [
    "git",
    "version",
    "mark",
    "cli",
    "web",
    "tag"
  ],
  "bin": {
    "vmark": "dist/cli.js"
  },
  "author": "clencat <2091927351@qq.com>",
  "license": "MIT",
  "packageManager": "pnpm@9.5.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a145789/version-mark"
  },
  "bugs": "https://github.com/a145789/version-mark/issues",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.2.0",
    "@types/semver": "^7.5.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "cleye": "^1.3.2",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.1",
    "rslog": "^1.2.2",
    "semver": "^7.6.3"
  }
}
